<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ir.hbm.bean.BodyTermBean" table="bodyterm" lazy="false" >
        
		<id name="term_id" type="long">
        	<column name="term_id" />
        </id>       

        <property name="term" type="java.lang.String">
			<column name="term" />
        </property>
        <property name="document_id" type="long">
			<column name="document_id" />
        </property>
        <property name="tf" type="int">
			<column name="tf" />
        </property>
   
	</class>
</hibernate-mapping>